<%= simple_form_for @picture, url: wizard_path, :html => { :multipart => true } do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>

  <%= @calc.first_step %>
  </br>
  <%= f.input :human_count, collection: @human_counts, :include_blank => false %>
  <%= f.input :size, collection: @sizes, :include_blank => false %>
  <%= f.input :frame, collection: @frames, :include_blank => false %>
  <%= f.simple_fields_for :picture_user_photo do |builder| %>
  	<%= builder.input :image, as: :file %>
  <% end %>
  <%= f.input :gel %>
  <%= f.input :comments, :as => :text, :input_html => { rows: 5 } %>
  <%= f.button :submit %>

<% end %>
