<%= simple_form_for @picture, url: wizard_path, :html => { :multipart => true } do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>

  <%= f.input :user_name %>
  <%= f.input :user_mname %>
  <%= f.input :user_surname %>
  <%= f.input :user_email %>
  <%= f.input :user_address %>
  <%= f.input :user_tel %>
  <%= f.submit nil %>

<% end %>
